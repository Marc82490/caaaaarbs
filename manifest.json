{
	"manifest_version": 2,
	"name": "Name Converter",
	"version": "1.0",
	"description": "Correct team abbreviations on demand.",
	"icons": { "16": "images/hockey_player_16.png",
           "48": "images/hockey_player_48.png",
          "128": "images/hockey_player_128.png" },
	"content_scripts":
	[
		{
			"matches": ["*://*.reddit.com/*"],
			"js": ["myscript.js"],
			"run_at": "document_end"
		}
	]
}
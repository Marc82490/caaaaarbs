{
	"manifest_version": 2,
	"name": "Name Converter",
	"version": "1.0",
	"description": "Correct team abbreviations on demand.",
	"icons": { "16": "images/image16.png",
           "48": "images/image48.png",
          "128": "images/image128.png" },
	"content_scripts":
	[
		{
			"matches": ["*://*.reddit.com/*"],
			"js": ["myscript.js"],
			"run_at": "document_end"
		}
	],
    "browser_action": {
      "default_icon": "images/image48.png"
    }
}